use proto::Numeric;

fn max<T: Numeric>(a: T, b: T) -> T {
    if a > b {
        a
    } else {
        b
    }
}

fn min<T: Numeric>(a: T, b: T) -> T {
    if a < b {
        a
    } else {
        b
    }
}

fn abs<T: Numeric>(a: T) -> T {
    if a < (0 as T) {
        (0 as T) - a
    } else {
        a
    }
}
