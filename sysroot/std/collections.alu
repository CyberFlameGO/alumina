//! Heap-allocating collections.

use vector::Vector;
use hashmap::HashMap;
use hashset::HashSet;
use deque::Deque;

/// Helper for collections that own heap-allocated objects. This is used to
/// be able to do `defer col.free_all()` without much boilerplate.
fn free_all<T: mem::Freeable<T>(col: &mut T) {
    for i in col {
        i.free();
    }
    col.free();
}
