{
  "name": "alumina",
  "displayName": "Alumina",
  "description": "Language support for ALumina",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "alumina",
        "aliases": [
          "Alumina",
          "alumina"
        ],
        "extensions": [
          ".alu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "alumina",
        "scopeName": "source.alumina",
        "path": "./syntaxes/alumina.tmLanguage.json"
      }
    ]
  }
}
